{
  "name": "julia",
  "version": "1.0.0",
  "description": "Julia's dashing website.",
  "scripts": {
    "clean": "rm -rf public/javascripts/* public/stylesheets/* .sass-cache *~ *.log",
    "buildjs": "browserify js/index.js -t babelify -o public/javascripts/bundle.js",
    "watchjs": "watchify js/index.js -t babelify -o public/javascripts/bundle.js",
    "buildcss": "sass --update css:public/stylesheets",
    "watchcss": "sass --watch css:public/stylesheets",
    "build": "npm run buildjs && npm run buildcss",
    "watch": "npm run watchjs & npm run watchcss",
    "dev": "npm run watch & python -m SimpleHTTPServer"
  },
  "author": "Julia Wang",
  "license": "ISC",
  "dependencies": {
    "alt": "0.17.1",
    "classnames": "^2.1.5",
    "react": "0.13.3"
  },
  "devDependencies": {
    "babelify": "6.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juliahw/jwang.git"
  }
}
