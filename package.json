{
  "name": "julia",
  "version": "1.0.0",
  "description": "Julia's dashing website.",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "scripts": {
    "clean": "rm -rf public/* .sass-cache *~ *.log",
    "build:js": "browserify js/index.js -d -o public/bundle.js -t [ babelify ]",
    "watch:js": "watchify js/index.js -d -o public/bundle.js -t [ babelify ]",
    "build:css": "sass --update css:public/",
    "watch:css": "sass --watch css:public/",
    "build": "npm run build:js && npm run build:css",
    "watch": "parallelshell 'npm run watch:js' 'npm run watch:css'",
    "livereload": "live-reload public/ --port=8080",
    "dev": "parallelshell 'npm run watch' 'npm run livereload' 'python -m SimpleHTTPServer'"
  },
  "author": "Julia Wang",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.7.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "live-reload": "^1.1.0",
    "parallelshell": "^2.0.0",
    "watchify": "^3.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juliahw/juliahw.github.io.git"
  }
}
